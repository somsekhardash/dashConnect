{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "baseUrl": "src/",
    },
    "paths": {
      "@actions": [
        "actions"
      ],
      "@actions/*": [
        "actions/*"
      ],
      "@components": [
        "components"
      ],
      "@components/*": [
        "components/*"
      ],
      "@util": [
        "utils"
      ],
      "@util/*": [
        "utils/*"
      ],
      "@models": [
        "models"
      ],
      "@models/*": [
        "models/*"
      ],
      "@reducers": [
        "reducers"
      ],
      "@reducers/*": [
        "reducers/*"
      ],
      "@store": [
        "store"
      ],
      "@store/*": [
        "store/*"
      ],
      
    },
    "include": [
        "./src/**/*"
    ]
}